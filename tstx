function gunwar(tedadDGL, refineNUMB)
    gg.setVisible(false)
    gg.searchNumber(tostring(tedadDGL), gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
    gg.refineNumber(tostring(refineNUMB), gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
    srch = gg.getResults(100, nil, nil, nil, nil, nil, nil, nil, nil)
    gg.toast(srch)
    gg.editAll("9999", gg.TYPE_DWORD)
    for i, v in ipairs(srch) do
    v.freeze = true
    end
    gg.toast("Cheat Activeâœ…")
    gg.clearResults()
    HOME()
end
